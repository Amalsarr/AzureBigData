{
	"name": "pipeline1",
	"properties": {
		"activities": [
			{
				"name": "Archive the data",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "BinarySource",
						"storeSettings": {
							"type": "AzureBlobFSReadSettings",
							"recursive": false
						},
						"formatSettings": {
							"type": "BinaryReadSettings"
						}
					},
					"sink": {
						"type": "BinarySink",
						"storeSettings": {
							"type": "AzureBlobFSWriteSettings"
						}
					},
					"enableStaging": false
				},
				"inputs": [
					{
						"referenceName": "ls_load_generic_binary",
						"type": "DatasetReference",
						"parameters": {
							"filePath": "Capstone2/incremetal_pl/posts_incremental/inbound/",
							"container": "mydatalakefilesystem"
						}
					}
				],
				"outputs": [
					{
						"referenceName": "ls_load_generic_binary",
						"type": "DatasetReference",
						"parameters": {
							"filePath": {
								"value": "@concat(\n    'Capstone2/incremetal_pl/posts_incremental/archive/',\n    formatDateTime(utcNow(), 'yyyy'),\n    '/',\n    formatDateTime(utcNow(), 'MM'),\n    '/',\n    formatDateTime(utcNow(), 'dd'),\n    '/'\n)",
								"type": "Expression"
							},
							"container": "mydatalakefilesystem"
						}
					}
				]
			}
		],
		"annotations": []
	}
}